    <div class="col-sm-6">
        <div class="white-box" style="height:507px">
            @*<h3 class="box-title">New Patient List</h3>
            <p class="text-muted">this is the sample data here for crm</p>*@

            <div class="r-icon-stats">
                <i class="ti-user bg-megna"></i>
                <div class="bodystate">
                    <h4>@Html.Raw(HttpContext.Current.Session["TotalDoctors"].ToString())</h4>
                    <span class="text-muted">Total Doctors</span>
                </div>
            </div>

            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>User Name</th>
                            <th>Full Name</th>
                            <th>Designation</th>
                            <th>Specialization</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            foreach (var item in (List<HospitalManagament.User>)HttpContext.Current.Session["TotalDoctorList"])
                            {
                                <tr>
                                    <td>@item.UserName</td>
                                    <td>@item.FullName</td>
                                    <td>@item.Doctor.Designation</td>
                                    <td>@item.Doctor.Specialization</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>

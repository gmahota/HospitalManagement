    <div class="col-sm-6">
        <div class="white-box" style="height:507px">
            @*<h3 class="box-title">New Patient List</h3>
            <p class="text-muted">this is the sample data here for crm</p>*@

            <div class="r-icon-stats">
                <i class="ti-user bg-megna"></i>
                <div class="bodystate">
                    <h4>@Html.Raw(HttpContext.Current.Session["TotalPatients"].ToString())</h4>
                    <span class="text-muted">Total Patients</span>
                </div>
            </div>

            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>User Name</th>
                            <th>Full Name</th>
                            <th>Occupation</th>
                            <th>Diseases</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            foreach (var item in (List<HospitalManagament.User>)HttpContext.Current.Session["TotalPatientList"])
                            {
                                <tr>
                                    <td>@item.UserName</td>
                                    <td>@item.FullName</td>
                                    <td>@item.Patient.Occupation</td>
                                    <td>@item.Patient.Disease</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

